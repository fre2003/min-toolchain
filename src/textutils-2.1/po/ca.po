#, c-format
msgid "%s: %s:%s: disorder: "
msgstr "%s: %s:%s: desordre: "

#: src/sort.c:1570
msgid "standard error"
msgstr "error estàndard"

#: src/sort.c:2028
#, c-format
msgid "%s: invalid field specification `%s'"
msgstr "%s: l'especifiació de camp no és vàlida: `%s'"

#: src/sort.c:2054
#, c-format
