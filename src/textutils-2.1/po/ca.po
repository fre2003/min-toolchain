msgstr "error d'escriptura"

#: src/sort.c:644
msgid "sort size"
msgstr "mida de l'ordenació"

#: src/sort.c:718
msgid "stat failed"
msgstr "error en l'estat"

#: src/sort.c:970
msgid "read failed"
msgstr "error de lectura"

